<h1>Produtos</h1>
<table>
    <thead>
        <tr>
            <th> Nome </th>
            <th> Valor </th>
            <th> Restaurante </th>
        </tr>
    </thead>
    <tbody>
        <% @products.each do |p| %>
            <tr>
            <th><%= p.name  %></th>
            <th><%= p.value  %></th>
            <th><%= p.restaurant.name %></th>
        <% end %>
    </tr>
    </tbody>
</table>
